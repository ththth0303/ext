{
  "manifest_version": 2,
  "name": "I want boob gif",
  "description": "This extension get a random boob gif for relaxing",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.gif",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "jquery-ui.js", "all.js", "js/externals/popup.js"],
      "css": ["jquery-ui.css",  "all.css"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "all.js"
  ]
}
