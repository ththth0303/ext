{
  "manifest_version": 2,
  "name": "I want boob gif",
  "description": "This extension get a random boob gif for relaxing",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/libraries/jquery.min.js", "js/libraries/jquery-ui.js", "js/internal/background.js"],
      "css": ["css/jquery-ui.css",  "css/background.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "js/internal/background.js"
  ]
}
